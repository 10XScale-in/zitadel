# Zitadel Instance URL (use container name for Docker networking)
INSTANCE_URL=http://localhost:8080

# Personal Access Token with IAM_OWNER permissions
PAT=your_pat_token_here

# Maximum concurrent sessions per user
CONCURRENT_SESSIONS=2

# Signing key from the Target (obtained after creating Target in Zitadel)
SIGNING_KEY=your_signing_key_here

# Server port
PORT=3001
